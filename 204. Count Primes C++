class Solution {
public:
    int countPrimes(int n) {
        if(n<=1)
            return 0;
        
        vector<int> prime(n,true);
        prime[0]= false;
        prime[1] = false;
        
        int sq=sqrt(n);
        for(int i=2;i<=sq;i++){
                        
            if(prime[i])
            for(int j=i*i;j<n;j+=i){
                prime[j] = false;
                
            }
        }
        
        return count(prime.begin(),prime.end(),true);
    }
};
